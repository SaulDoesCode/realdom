{
  "version": "1.0.0",
  "name": "doc",
  "description": "lightweight DOM Manipulation",
  "repository": {
    "type": "git",
    "url": "https://github.com/ndaidong/doc"
  },
  "author": "@ndaidong",
  "main": "./index.js",
  "engines": {
    "node": ">= 6.0"
  },
  "scripts": {
    "lint": "eslint .",
    "snyk": "snyk test",
    "karma": "karma start test/karma.conf.js",
    "pretest": "npm run lint && npm run snyk",
    "test": "npm run karma",
    "build": "gccmin"
  },
  "devDependencies": {
    "eslint": "latest",
    "eslint-config-ggc": "latest",
    "gcc-min": "latest",
    "jasmine-core": "latest",
    "karma": "latest",
    "karma-chrome-launcher": "latest",
    "karma-firefox-launcher": "latest",
    "karma-jasmine": "latest",
    "karma-opera-launcher": "latest",
    "snyk": "latest"
  },
  "gccmin": {
    "source": "src",
    "target": "dist",
    "filename": "doc",
    "globalVar": "doc"
  },
  "keywords": [
    "dom",
    "event",
    "browser",
    "manipulation",
    "util"
  ],
  "license": "MIT"
}
